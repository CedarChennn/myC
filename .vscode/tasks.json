{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "WSL default build active file",//"label": "C/C++: g++ build active file",
			"command": "/usr/bin/g++",
			"args": [
				"-std=c++17",
				"-g",
				"${file}",
				"-o",
				//"${fileDirname}/${fileBasenameNoExtension}",
				"${workspaceFolder}/test.out",

				
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}